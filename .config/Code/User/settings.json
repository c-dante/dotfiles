{
    "todohighlight.isCaseSensitive": false,
    "todohighlight.keywordsPattern": "@?todo(\\n|\\W[^\\n]*)",
    "todohighlight.defaultStyle": {
        // "border": "2px solid #AA00AA",
        // "borderRadius": "5px",
        "outline": "1px solid #AA00AA",
        "color": "editor.foregroundColor",
        "backgroundColor": "editor.backgroundColor"
    },
    "editor.fontFamily": "'Source Code Pro', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.fontSize": 12,
    "editor.renderWhitespace": "all",
    "editor.tabSize": 2,
    "editor.insertSpaces": false,
    "editor.minimap.enabled": false,
    "files.trimTrailingWhitespace": true,
    "explorer.openEditors.visible": 0,
    "gitlens.advanced.messages": {
        "suppressShowKeyBindingsNotice": true,
        "suppressWelcomeNotice": true
    },
    "files.exclude": {
        "**/node_modules": true,
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true
    },
    "gitlens.keymap": "none",
    "gitlens.gitExplorer.enabled": false,
    "gitlens.historyExplorer.enabled": false,
    "gitlens.codeLens.enabled": false,
    "gitlens.mode.statusBar.enabled": false,
    "extensions.ignoreRecommendations": false,
    "eslint.autoFixOnSave": true,
    "cSpell.dictionaryDefinitions": [
        "Pred"
    ],
    "telemetry.enableTelemetry": false,
    "javascript.preferences.quoteStyle": "single",
    "editor.quickSuggestionsDelay": 100,
    "editor.snippetSuggestions": "none",
    "editor.acceptSuggestionOnCommitCharacter": false,
    "editor.parameterHints.enabled": false,
    "editor.lightbulb.enabled": false,
    "editor.dragAndDrop": false,
    "window.zoomLevel": 0,
    "sync.gist": "6aae7f04d52d5a25a1288d62d93989f0",
}
