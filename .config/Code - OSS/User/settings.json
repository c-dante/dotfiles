{
    "todohighlight.isCaseSensitive": false,
    "todohighlight.keywordsPattern": "@?todo(\\n|\\W[^\\n]*)",
    "todohighlight.defaultStyle": {
        // "border": "2px solid #AA00AA",
        // "borderRadius": "5px",
        "outline": "1px solid #AA00AA",
        "color": "editor.foregroundColor",
        "backgroundColor": "editor.backgroundColor"
    },
    "editor.fontFamily": "'Source Code Pro', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.renderWhitespace": "all",
    "editor.tabSize": 2,
    "editor.insertSpaces": false,
    "editor.minimap.enabled": false,
    "files.trimTrailingWhitespace": true,
    "explorer.openEditors.visible": 0,
    "gitlens.advanced.messages": {
        "suppressLineUncommittedWarning": true,
        "suppressShowKeyBindingsNotice": true,
        "suppressWelcomeNotice": true
    },
    "files.exclude": {
        "**/node_modules": true,
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true
    },
    "gitlens.keymap": "none",
    "gitlens.gitExplorer.enabled": false,
    "gitlens.historyExplorer.enabled": false,
    "gitlens.codeLens.enabled": false,
    "gitlens.mode.statusBar.enabled": false,
    "extensions.ignoreRecommendations": false,
    "eslint.autoFixOnSave": true,
    "cSpell.dictionaryDefinitions": [
        "Pred"
    ],
    "telemetry.enableTelemetry": false,
    "telemetry.enableCrashReporter": false,
    "editor.quickSuggestionsDelay": 100,
    "editor.snippetSuggestions": "none",
    "editor.acceptSuggestionOnCommitCharacter": false,
    "editor.parameterHints.enabled": false,
    "editor.lightbulb.enabled": false,
    "editor.dragAndDrop": false,
    "window.zoomLevel": 0,
    "sync.gist": "6aae7f04d52d5a25a1288d62d93989f0",
    "breadcrumbs.enabled": true,
    "gitlens.currentLine.enabled": false,
    "gitlens.hovers.currentLine.over": "line",
    "gitlens.hovers.enabled": false,
    "gitlens.statusBar.reduceFlicker": true,
    "gitlens.views.repositories.enabled": false,
    "gitlens.views.fileHistory.enabled": false,
    "gitlens.views.lineHistory.enabled": false,
    "gitlens.views.search.enabled": false,
    "gitlens.views.compare.enabled": false,
    "gitlens.blame.format": "${author} : ${message|40?} ${agoOrDate|14-}",
    "gitlens.blame.avatars": false,
    "gitlens.blame.heatmap.location": "left",
    "gitlens.blame.highlight.locations": [
        "line",
        "overview",
        "gutter"
    ],
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "gitlens.advanced.telemetry.enabled": false,
    "typescript.surveys.enabled": false,
    "javascript.preferences.quoteStyle": "single",
    "typescript.preferences.quoteStyle": "single",
}
